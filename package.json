    "react-dom": "^19.1.0",
    "react-dropzone": "^14.3.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.2.1",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "buffer": "^6.0.3",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
